import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Date;
import javax.xml.crypto.Data;

public class Coupon {
	private Integer COUid;
	private String COUtitle;
	private Date date = new Date();
	private Calendar cal = Calendar.getInstance();
	private Integer COUammount;
	private string COUmessage;
	private double COUprice;
	private string COUimage;

	private enum string {
		food, electricity, fun, vication, shopping

	}

	public Coupon(Integer cOUid, String cOUtitle, Date date, Calendar cal, Integer cOUammount, string cOUmessage,
			double cOUprice, string cOUimage) {
		COUid = cOUid;
		COUtitle = cOUtitle;
		this.date = date;
		this.cal = cal;
		COUammount = cOUammount;
		COUmessage = cOUmessage;
		COUprice = cOUprice;
		COUimage = cOUimage;

		cal.setTime(date);
		cal.add(Calendar.MONTH, 6);

	}

	private Integer getCOUid() {
		return COUid;
	}

	private void setCOUid(Integer cOUid) {
		COUid = cOUid;
	}

	private String getCOUtitle() {
		return COUtitle;
	}

	private void setCOUtitle(String cOUtitle) {
		COUtitle = cOUtitle;
	}

	private Date getDate() {
		return date;
	}

	private void setDate(Date date) {
		this.date = date;
	}

	private Calendar getCal() {
		return cal;
	}

	private void setCal(Calendar cal) {
		this.cal = cal;
	}

	private Integer getCOUammount() {
		return COUammount;
	}

	private void setCOUammount(Integer cOUammount) {
		COUammount = cOUammount;
	}

	private string getCOUmessage() {
		return COUmessage;
	}

	private void setCOUmessage(string cOUmessage) {
		COUmessage = cOUmessage;
	}

	private double getCOUprice() {
		return COUprice;
	}

	private void setCOUprice(double cOUprice) {
		COUprice = cOUprice;
	}

	private string getCOUimage() {
		return COUimage;
	}

	private void setCOUimage(string cOUimage) {
		COUimage = cOUimage;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((COUid == null) ? 0 : COUid.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Coupon other = (Coupon) obj;
		if (COUid == null) {
			if (other.COUid != null)
				return false;
		} else if (!COUid.equals(other.COUid))
			return false;
		return true;
	}

	@Override
	public String toString() {
		return "Coupon [COUid=" + COUid + ", COUtitle=" + COUtitle + ", date=" + date + ", cal=" + cal + ", COUammount="
				+ COUammount + ", COUmessage=" + COUmessage + ", COUprice=" + COUprice + ", COUimage=" + COUimage + "]";
	}

}
